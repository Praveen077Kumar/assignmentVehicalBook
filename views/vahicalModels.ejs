<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/wheelsNo.css">
  <title>Vehicle Rental</title>
</head>

<body>

  <div class="container">
    <h2 class="main-heading">Select Model</h2>
   <form action="bookingdate" method="POST"> 
           <% if (models.length > 0) { %> 
           <div>
            <% for (let model of models) { %>
            <input type="radio" id="model" name="vahicalModel" value=<%= model.model %> /><%= model.model%>
            <br>
             <% } %>
           </div>
           <% } else { %> 
                        <h1>No Products Found!</h1>
                  <% } %>

        <div class="pagination">
              <button class="btn" id="previous">Prev</button>
              <input type="submit" id="next" value="Next" class="btn">
          </div>  
    </form>  
  </div>
</body>
  <script src="js/script.js"> </script>

</html>